<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="org.khj.mapper.smsMemberMapper">
	   <insert id="insert">
	   		insert into member
	   		values(#{id},#{password},#{name},#{phone})
	   </insert>
   
		<select id="login" resultType="org.khj.domain.smsMemberDTO">
		  		select *
		  		from member
		  		where id=#{id}
		  		and password=#{password}
		</select>

		<!--  
		<select id="confirmId" resultType="int">
		  		select count(*)
		  		from member
		  		where id=#{id}
		</select>
		 -->		
   </mapper>